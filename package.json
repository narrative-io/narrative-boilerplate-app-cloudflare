        "vue": "^2.6.11",
        "vue-router": "^3.6.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.6",
        "@vue/cli-plugin-eslint": "~4.5.6",
        "@vue/cli-service": "~4.5.13",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "sass": "~1.54.5",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.11",
